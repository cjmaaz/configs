// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+b",
    "command": "-workbench.action.quickOpenNavigateNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+b",
    "command": "-workbench.action.showAllEditors"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "tab",
    "command": "-editor.emmet.action.expandAbbreviation",
    "when": "config.emmet.triggerExpansionOnTab && editorTextFocus && !editorReadonly && !editorTabMovesFocus"
  },
  {
    "key": "ctrl+e ctrl+m",
    "command": "editor.emmet.action.expandAbbreviation",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+m",
    "command": "sfdx.force.source.deploy.in.manifest"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+f",
    "command": "sfdx.force.source.deploy.current.source.file"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+d",
    "command": "sfdx.force.source.deploy.source.path"
  },
  {
    "key": "ctrl+alt+c",
    "command": "notifications.clearAll"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+r",
    "command": "sfdx.force.anon.apex.execute.selection"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+e",
    "command": "sfdx.force.anon.apex.execute.document"
  },
  {
    "key": "ctrl+shift+s ctrl+shift+q",
    "command": "sfdx.force.data.soql.query.selection"
  },
  {
    "key": "ctrl+shift+delete",
    "command": "workbench.output.action.clearOutput"
  }
]
