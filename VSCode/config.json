  "vsintellicode.modify.editor.suggestSelection": "enabled",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffe657",
    "terminalCursor.foreground": "#ff0a54",
    "editorIndentGuide.activeBackground": "#ff0000",
    "editorIndentGuide.background": "#555",
    "[Material Theme Darker]": {
      "activityBarBadge.background": "#2979FF",
      "activityBar.activeBorder": "#2979FF",
      "list.activeSelectionForeground": "#2979FF",
      "list.inactiveSelectionForeground": "#2979FF",
      "list.highlightForeground": "#2979FF",
      "scrollbarSlider.activeBackground": "#2979FF50",
      "editorSuggestWidget.highlightForeground": "#2979FF",
      "textLink.foreground": "#2979FF",
      "progressBar.background": "#2979FF",
      "pickerGroup.foreground": "#2979FF",
      "tab.activeBorder": "#2979FF",
      "notificationLink.foreground": "#2979FF",
      "editorWidget.resizeBorder": "#2979FF",
      "editorWidget.border": "#2979FF",
      "settings.modifiedItemIndicator": "#2979FF",
      "settings.headerForeground": "#2979FF",
      "panelTitle.activeBorder": "#2979FF",
      "breadcrumb.activeSelectionForeground": "#2979FF",
      "menu.selectionForeground": "#2979FF",
      "menubar.selectionForeground": "#2979FF",
      "editor.findMatchBorder": "#2979FF",
      "selection.background": "#2979FF40",
      "statusBarItem.remoteBackground": "#2979FF"
    }
  },
  "extensions.ignoreRecommendations": true,
  "workbench.sideBar.location": "right", //* SideBar on the RIGHT
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.colorTheme": "Monokai++ Unified",
  // Startup Name
  "workbench.startupEditor": "newUntitledFile",

  // Icon
  "workbench.iconTheme": "material-icon-theme",

  // Editor to split vertically on right or below horizontally
  "workbench.editor.splitInGroupLayout": "vertical",
  "workbench.enableExperiments": false,

  // _________________________________________________________________________
  //? _______________________EDITIOR SETTINGS____________________________
  "editor.stickyScroll.enabled": false, //5 Sep-2022
  "editor.stickyScroll.maxLineCount": 5,
  "editor.tabSize": 2, //5 Sep-2022
  "editor.hover.sticky": true, //5 Sep-2022 Keep popup open if moused move over it
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true, //* Show Suggestions
  "editor.parameterHints.enabled": true,
  "editor.hover.delay": 700,
  "editor.selectionHighlight": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoIndent": "brackets",
  "editor.cursorStyle": "line", //* Straight line style for cursor
  "editor.dragAndDrop": false, //* Not let select and move code through Mouse
  "editor.codeLens": true, //* Code Lens
  "editor.renderLineHighlight": "all",
  //"editor.tabSize": 4,
  /*"editor.insertSpaces": true,
  "editor.detectIndentation": false,*/

  // Disable Mouse Scroll
  "editor.roundedSelection": true,
  "editor.mouseWheelZoom": false,
  //"editor.mouseWheelZoom": true,
  "terminal.integrated.cursorBlinking": true,
  // Formatting
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  // Word-wrap
  "editor.wordWrap": "off",
  // Windows Title
  // "window.title": "üëâüèª ${dirty}${activeEditorShort} ‚ùï ${rootName} üìÇ ",
  "window.title": "${rootName}üóÉÔ∏è${separator}${dirty}${activeEditorShort}üëàüèª ${activeFolderShort}üìÇ WORKüßëüèª‚Äçüíª",
  //"window.title": "${rootName}üóÉÔ∏è${separator}${dirty}${activeEditorShort}üëàüèª ${activeFolderShort}üìÇ PERSONALüßëüèª‚Äçüíª",

  //Fonts
  "editor.fontSize": 20,
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.fontFamily": "'Cascadia Code','Fira Code',Consolas",
  "editor.codeLensFontFamily": "'Cascadia Code','Fira Code',Consolas", //* Side Map
  //Glyph for Debugger
  "editor.lineDecorationsWidth": 0,
  "editor.glyphMargin": false,
  "editor.linkedEditing": true,
  //Cursor
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,

  // Optimization
  "editor.largeFileOptimizations": false,

  //Smooth Scroll
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": false,

  //Mini Map
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": false,

  "editor.accessibilitySupport": "off",

  // Partial Cursive and other settings
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type.js",
          "storage.type.function.js",
          "entity.other.attribute-name.html",
          "string.quoted.single.js",
          "string.quoted.double.js",
          "string.template.js",
          "string.quoted.single.apex",
          "string.quoted.double.apex"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "keyword.control.conditional",
          "keyword.operator",
          "storage.type.function.arrow.js"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["support.function.js"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },

  //_________________________________________________________________________
  //? __________________________VS Code OWN COLORIZE____________________________
  "editor.bracketPairColorization.enabled": true,

  //___________________________________________________________________________
  //?_____________________________INDENT RAINBOW________________________________

  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 25,

  //____________________________________________________________________________
  //? _____________________LANGUAGE SPECIFIC SETTINGS__________________________
  "jshint.options": {
    "esversion": 9
  },

  "jshint.lintHTML": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    //"editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.trimAutoWhitespace": false
  },
  "html.format.indentInnerHtml": true,
  "html.format.extraLiners": "true",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.autoClosingQuotes": "always",
    "editor.defaultFormatter": "vscode.html-language-features"
    //"editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "vetur.format.defaultFormatter.html": "prettier",
  "json.maxItemsComputed": 7000,
  "html.format.wrapLineLength": 0,
  "xml.completion.autoCloseTags": true,
  //*  JSHint
  "jshint.enable": false,

  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",

  // Salesforce Configs:
  "[apex]": {
    "editor.defaultFormatter": "vscjava.vscode-java-debug"
  },
  "salesforcedx-vscode-core.detectConflictsAtSync": true, // Sync Settings

